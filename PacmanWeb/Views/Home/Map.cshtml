@{
    ViewData["Title"] = "Map";
}
<h6></h6>
<link rel="stylesheet" type="text/css" href="~/css/game.css" />
<img src="~/images/allelements/blinky.png" id="blinky" hidden="hidden">
<img src="~/images/allelements/frightened.png" id="frightened" hidden="hidden" />
<img src="~/images/allelements/emtry.jpg" id="emtry" hidden="hidden" />
<img src="~/images/allelements/clyde.png" id="clyde" hidden="hidden">
<img src="~/images/allelements/inky.png" id="inky" hidden="hidden">
<img src="~/images/allelements/pinky.png" id="pinky" hidden="hidden">
<img src="~/images/allelements/pacmanleft.png" id="pacman" hidden="hidden">
<img src="~/images/allelements/pacmanleft.png" id="pacmanleft" hidden="hidden">
<img src="~/images/allelements/pacmanup.png" id="pacmanup" hidden="hidden">
<img src="~/images/allelements/pacmanright.png" id="pacmanright" hidden="hidden">
<img src="~/images/allelements/pacmandown.png" id="pacmandown" hidden="hidden">
<img src="~/images/allelements/cherry.png" id="cherry" hidden="hidden">
<img src="~/images/allelements/energaizer.png" id="energaizer" hidden="hidden">
<img src="~/images/allelements/littlegoal.png" id="littlegoal" hidden="hidden">

<div id="all">
    <div>
        <div>
            <div id="information">
                <h4>Level: </h4>
                <h4 id="level">1</h4>
            </div>
            <div id="information">
                <h4>Score: </h4>
                <h4 id="score">0</h4>
            </div>
            <div id="information">
                <h4>Live: </h4>
                <h4 id="live">3</h4>
            </div>
        </div>
    </div>
    <div id="game">
        <canvas id="map" height="589" width="570"></canvas>
    </div>
    <div id="buttons">
        <form>
            <input type="submit" id="start" value="Start" />
        </form>
        <form>
            <input type="submit" id="stop" value="Stop" />
        </form>
    </div>
</div>
<script src="~/js/drawmap.js"></script>
<script src="~/js/initmap.js"></script>
<script src="~/lib/signalr.min.js"></script>
<script src="~/js/movement.js"></script>
